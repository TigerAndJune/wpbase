{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.7.8",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download"
        },
        "require": {
          "philippbaschke/acf-pro-installer": "^1.0",
          "composer/installers": "^1.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "1.*",
    "johnpbloch/wordpress": "4.4.*",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/duplicator": "1.2.52",
    "wpackagist-plugin/custom-post-type-ui": "1.6.1",
    "wpackagist-theme/twentysixteen": "*",
    "philippbaschke/acf-pro-installer": "^1.0",
    "wpackagist-plugin/timber-library": "1.8.3",
    "wpackagist-plugin/woocommerce": "3.5.2",
    "wpackagist-plugin/contact-form-7": "5.1",
    "wpackagist-plugin/contact-form-cfdb7": "1.2.4.5"
  },
  "suggest": {
  },
  "extra": {
    "wordpress-install-dir": "public/cms",
    "installer-paths": {
      "public/content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "public/content/plugins/{$name}/": ["type:wordpress-plugin"],
      "public/content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
